{
	"name": "freelancers-calendar",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@reach/router": "latest",
		"axios": "^0.19.0",
		"http-proxy-middleware": "^0.20.0",
		"lodash": "^4.17.15",
		"nanoid": "^2.1.3",
		"query-string": "^6.8.3",
		"react": "^16.9.0",
		"react-dom": "^16.9.0",
		"react-scripts": "^3.1.1"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"analyze": "source-map-explorer build/static/js/*.js",
		"coverage": "react-scripts test --env=jsdom --coverage --watchAll=false",
		"server": "cd serv && yarn run dev",
		"dev": "concurrently \"yarn run server\" \"yarn run start\"",
		"prod": "cd serv && yarn run prod",
		"deploy": "yarn run build && yarn run prod"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"devDependencies": {
		"concurrently": "^5.0.0",
		"enzyme": "^3.10.0",
		"enzyme-adapter-react-16": "^1.15.1",
		"enzyme-to-json": "^3.4.2",
		"source-map-explorer": "^2.1.2"
	},
	"proxy": "http://localhost:5000",
	"jest": {
		"collectCoverageFrom": [
			"src/App/**/*.{js,jsx,tx,tsx}"
		]
	},
	"eslintConfig": {
		"extends": [
			"react-app"
		],
		"globals": {
			"shallow": "readonly",
			"render": "readonly",
			"mount": "readonly"
		}
	}
}
